<%= form_with model: @club_team do |f| %>
  <section class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control" %>
  </section>
  <section class="form-group">
    <%= f.label :city %>
    <%= f.text_field :city, class: "form-control" %>
  </section>
  <section class="form-group">
    <%= f.label :state %>
    <%= f.text_field :state, class: "form-control" %>
  </section>
  <%= f.submit nil, class: "btn btn-primary" %>
<% end %>
